+#!/usr/bin/env python3
+# -*- coding: utf-8 -*-
+
+"""
+Scalability Plot Maker — Chiaro e leggibile
+
+OBIETTIVO
+- Mostrare, per ciascun λ, come il tempo di attesa medio (edge_avg_wait) varia
+  al crescere del numero di nodi Edge (edge_server_number).
+- Esaltare:
+  1) Trend medio (linea collegando i #nodi) con intervalli di confidenza 95%
+  2) Benefici marginali (delta) passando da n nodi a n+1 nodi
+  3) Variabilità per #nodi (opzionale: boxplot per slot λ)
+
+USO SEMPLICE (default):
+    python utils/scalability_plot_maker.py
+
+USO PERSONALIZZATO:
+    python utils/scalability_plot_maker.py ^
+        --csv output/edge_scalability_statistics.csv ^
+        --out output/plots_scalability_lambda ^
+        --lambda-col lambda ^
+        --x-col edge_server_number ^
+        --y-col edge_avg_wait ^
+        --with-boxplot
+"""
+
+import os
+import sys
+import argparse
+from typing import Optional, List
+import numpy as np
+import pandas as pd
+import matplotlib.pyplot as plt
+from math import sqrt
+
+
+# -------------------------------
+# Utilità statistiche
+# -------------------------------
+
+def ensure_numeric(series: pd.Series) -> pd.Series:
+    return pd.to_numeric(series, errors="coerce")
+
+def ci95_halfwidth(values: np.ndarray) -> float:
+    """Half-width CI95% della media usando z≈1.96 (ok per n≥30; per n piccoli è indicativa)."""
+    n = len(values)
+    if n <= 1:
+        return 0.0
+    sd = float(np.std(values, ddof=1))
+    return 1.96 * sd / sqrt(n)
+
+
+# -------------------------------
+# Core
+# -------------------------------
+
+def build_summary(df: pd.DataFrame, x_col: str, y_col: str, lambda_col: str) -> pd.DataFrame:
+    """
+    Aggrega per (lambda, #nodi): mean, ci95, count, median.
+    Ritorna un DataFrame ordinato, pronto per plotting e report.
+    """
+    parts = []
+    for lam, g_lam in df.groupby(lambda_col):
+        for x, g_x in g_lam.groupby(x_col):
+            y = g_x[y_col].to_numpy()
+            parts.append({
+                lambda_col: lam,
+                x_col: x,
+                "mean": float(np.mean(y)),
+                "median": float(np.median(y)),
+                "count": int(len(y)),
+                "ci95": ci95_halfwidth(y),
+            })
+    out = pd.DataFrame(parts)
+    if out.empty:
+        return out
+    out = out.sort_values([lambda_col, x_col]).reset_index(drop=True)
+    return out
+
+
+def print_marginal_benefits(summary: pd.DataFrame, x_col: str, lambda_col: str):
+    """
+    Stampa i benefici marginali (delta medio di attesa) aggiungendo un nodo:
+    Δ = mean_wait(n) - mean_wait(n+1)  (positivo = miglioramento).
+    """
+    if summary.empty:
+        print("Nessun dato per calcolare i benefici marginali.")
+        return
+    print("\n== Benefici marginali (Δ attesa media passando da n a n+1 nodi) ==")
+    for lam, g in summary.groupby(lambda_col):
+        g = g.sort_values(x_col)
+        deltas = []
+        xs = g[x_col].tolist()
+        means = g["mean"].tolist()
+        for i in range(len(xs) - 1):
+            delta = means[i] - means[i + 1]
+            deltas.append((xs[i], xs[i + 1], delta))
+        print(f"λ={lam}:")
+        if deltas:
+            for a, b, d in deltas:
+                print(f"  {a}→{b} nodi: Δ={d:.6f} (positivo=attesa diminuisce)")
+        else:
+            print("  Solo un valore di nodi: impossibile valutare il trend.")
+
+
+def plot_line_with_ci_per_lambda(summary: pd.DataFrame,
+                                 out_dir: str,
+                                 x_col: str,
+                                 y_label: str,
+                                 lambda_col: str):
+    """
+    Per ogni λ crea un grafico:
+    - X: #nodi (discreto)
+    - Y: media attesa, con barre CI95%
+    - Linea collega i punti in ordine crescente di #nodi
+    - Annotazione del numero di campioni per punto
+    """
+    os.makedirs(out_dir, exist_ok=True)
+    if summary.empty:
+        print("Nessun dato da plottare.")
+        return
+
+    for lam, g in summary.groupby(lambda_col):
+        g = g.sort_values(x_col)
+
+        plt.figure(figsize=(10, 6))
+        # Linea delle medie
+        plt.plot(g[x_col].to_numpy(), g["mean"].to_numpy(), marker="o", linewidth=2)
+
+        # Barre di errore (CI95%)
+        plt.errorbar(
+            g[x_col].to_numpy(),
+            g["mean"].to_numpy(),
+            yerr=g["ci95"].to_numpy(),
+            fmt="none",
+            elinewidth=1,
+            capsize=3
+        )
+
+        # Annotazioni: n (conteggio) sopra ad ogni punto
+        for xi, yi, n in zip(g[x_col], g["mean"], g["count"]):
+            plt.annotate(f"n={n}", (xi, yi), textcoords="offset points", xytext=(0, 8), ha="center", fontsize=9)
+
+        plt.xlabel(x_col)
+        plt.ylabel(y_label)
+        plt.title(f"Impatto della scalabilità (Edge) sul tempo di attesa — λ={lam}")
+        plt.grid(True)
+
+        out_path = os.path.join(out_dir, f"scalability_lambda_{str(lam).replace('.', '_')}.png")
+        plt.savefig(out_path, dpi=150, bbox_inches="tight")
+        plt.close()
+
+
+def plot_optional_boxplots(df: pd.DataFrame,
+                           out_dir: str,
+                           x_col: str,
+                           y_col: str,
+                           lambda_col: str):
+    """
+    (Opzionale) Per ogni λ, boxplot dei tempi di attesa raggruppati per #nodi.
+    Utile per mostrare la variabilità (dispersione).
+    """
+    os.makedirs(out_dir, exist_ok=True)
+    for lam, g in df.groupby(lambda_col):
+        g = g.copy()
+        levels = sorted(g[x_col].dropna().unique().tolist())
+        if not levels:
+            continue
+        data = [g.loc[g[x_col] == lv, y_col].dropna().to_numpy() for lv in levels]
+
+        plt.figure(figsize=(10, 6))
+        plt.boxplot(data, labels=[str(lv) for lv in levels], showmeans=True)
+        plt.xlabel(x_col)
+        plt.ylabel(y_col)
+        plt.title(f"Variabilità del tempo di attesa per #nodi — λ={lam}")
+        plt.grid(True)
+
+        out_path = os.path.join(out_dir, f"boxplot_lambda_{str(lam).replace('.', '_')}.png")
+        plt.savefig(out_path, dpi=150, bbox_inches="tight")
+        plt.close()
+
+
+def run(csv_path: str,
+        out_dir: str,
+        x_col: str,
+        y_col: str,
+        lambda_col: str,
+        with_boxplot: bool):
+
+    if not os.path.isfile(csv_path):
+        raise FileNotFoundError(f"CSV non trovato: {csv_path}")
+
+    df = pd.read_csv(csv_path)
+
+    required = {"seed", x_col, y_col, lambda_col}
+    missing = [c for c in required if c not in df.columns]
+    if missing:
+        raise ValueError(f"Mancano colonne richieste nel CSV: {missing}")
+
+    # Cast numerici
+    df[x_col] = ensure_numeric(df[x_col])
+    df[y_col] = ensure_numeric(df[y_col])
+    df[lambda_col] = ensure_numeric(df[lambda_col])
+
+    # Drop NA essenziali
+    df = df.dropna(subset=[x_col, y_col, lambda_col])
+
+    # Summary per plotting
+    summary = build_summary(df, x_col=x_col, y_col=y_col, lambda_col=lambda_col)
+
+    # Salva summary per analisi
+    os.makedirs(out_dir, exist_ok=True)
+    summary_path = os.path.join(out_dir, "scalability_summary.csv")
+    summary.to_csv(summary_path, index=False)
+
+    # Plot principali (linea + CI)
+    plot_line_with_ci_per_lambda(
+        summary=summary,
+        out_dir=out_dir,
+        x_col=x_col,
+        y_label=y_col,
+        lambda_col=lambda_col
+    )
+
+    # Benefici marginali a console
+    print_marginal_benefits(summary, x_col=x_col, lambda_col=lambda_col)
+
+    # Opzionale: boxplot
+    if with_boxplot:
+        plot_optional_boxplots(
+            df=df,
+            out_dir=os.path.join(out_dir, "boxplots"),
+            x_col=x_col,
+            y_col=y_col,
+            lambda_col=lambda_col
+        )
+
+    print(f"\n✅ Grafici e report salvati in: {out_dir}")
+    print(f"   - CSV riassuntivo: {summary_path}")
+
+
+# -------------------------------
+# CLI
+# -------------------------------
+
+def parse_args(argv: Optional[List[str]] = None) -> argparse.Namespace:
+    p = argparse.ArgumentParser()
+    p.add_argument("--csv", default="output/edge_scalability_statistics.csv",
+                   help="Percorso al CSV (default: output/edge_scalability_statistics.csv)")
+    p.add_argument("--out", default="output/plots_scalability_lambda",
+                   help="Cartella di output (default: output/plots_scalability_lambda)")
+    p.add_argument("--lambda-col", default="lambda",
+                   help="Nome colonna per λ (default: lambda)")
+    p.add_argument("--x-col", default="edge_server_number",
+                   help="Asse X: numero di nodi Edge (default: edge_server_number)")
+    p.add_argument("--y-col", default="edge_avg_wait",
+                   help="Asse Y: tempo di attesa medio (default: edge_avg_wait)")
+    p.add_argument("--with-boxplot", action="store_true",
+                   help="Se presente, genera anche boxplot di variabilità per λ.")
+    return p.parse_args(argv)
+
+
+def main():
+    args = parse_args(sys.argv[1:])
+    run(csv_path=args.csv,
+        out_dir=args.out,
+        x_col=args.x_col,
+        y_col=args.y_col,
+        lambda_col=args.lambda_col,
+        with_boxplot=args.with_boxplot)
+
+
+if __name__ == "__main__":
+    main()
